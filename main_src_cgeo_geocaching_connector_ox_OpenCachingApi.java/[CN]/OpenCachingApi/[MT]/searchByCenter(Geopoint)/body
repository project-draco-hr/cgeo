{
  final Parameters queryParameters=new Parameters("log_limit","0","hint","false","description","none","limit","20","center",center.format(GeopointFormatter.Format.LAT_LON_DECDEGREE_COMMA));
  if (addTypeFilter(queryParameters)) {
    final HttpResponse response=getRequest(API_URL_CACHES_GPX,queryParameters);
    return importCachesFromResponse(response,false);
  }
  return Collections.emptyList();
}
