{
  getPreference(R.string.pref_fakekey_basicmembers_screen).setEnabled(!Settings.isPremiumMember());
  getPreference(R.string.pref_loaddirectionimg).setEnabled(!Settings.isPremiumMember());
  getPreference(R.string.pref_showcaptcha).setEnabled(!Settings.isPremiumMember());
