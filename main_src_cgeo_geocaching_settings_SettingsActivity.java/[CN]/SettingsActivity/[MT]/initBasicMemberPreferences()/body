{
  getPreference(R.string.pref_fakekey_basicmembers_screen).setEnabled(!Settings.isGCPremiumMember());
  getPreference(R.string.pref_loaddirectionimg).setEnabled(!Settings.isGCPremiumMember());
  getPreference(R.string.pref_showcaptcha).setEnabled(!Settings.isGCPremiumMember());
