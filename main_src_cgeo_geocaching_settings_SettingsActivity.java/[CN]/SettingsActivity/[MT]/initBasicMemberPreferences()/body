{
  SettingsActivity.findPreference(this,getKey(R.string.pref_fakekey_basicmembers_screen)).setEnabled(!Settings.isPremiumMember());
  SettingsActivity.findPreference(this,getKey(R.string.pref_loaddirectionimg)).setEnabled(!Settings.isPremiumMember());
  SettingsActivity.findPreference(this,getKey(R.string.pref_showcaptcha)).setEnabled(!Settings.isPremiumMember());
  redrawScreen(R.string.pref_fakekey_services_screen);
}
