{
  if (CollectionUtils.isEmpty(list) || selectMode) {
    return;
  }
  if (isSortedByDistance()) {
    if (coordsIn == null) {
      return;
    }
    Collections.sort(list,new DistanceComparator(coordsIn,list));
  }
 else {
    Collections.sort(list,cacheComparator);
  }
  notifyDataSetChanged();
}
