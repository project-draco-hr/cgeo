{
  if (list.isEmpty()) {
    return false;
  }
  if (checked <= 0) {
    return false;
  }
  boolean status=getSelectMode();
  int cleared=0;
  for (  cgCache cache : list) {
    if (cache.isStatusChecked()) {
      cache.setStatusChecked(false);
      checkChecked(-1);
      cleared++;
    }
  }
  setSelectMode(false,false);
  notifyDataSetChanged();
  return cleared > 0 || status;
}
