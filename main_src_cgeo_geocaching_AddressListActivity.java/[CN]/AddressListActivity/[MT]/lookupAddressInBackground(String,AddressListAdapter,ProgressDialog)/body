{
  new AsyncTask<Void,Void,List<Address>>(){
    @Override protected List<Address> doInBackground(    final Void... params){
      final Geocoder geocoder=new Geocoder(AddressListActivity.this);
      return geocoder.getFromLocationName(keyword);
    }
    @Override protected void onPostExecute(    final List<Address> addresses){
      waitDialog.dismiss();
      if (CollectionUtils.isNotEmpty(addresses)) {
        for (        final Address address : addresses) {
          adapter.add(address);
        }
      }
 else {
        finish();
        CacheListActivity.startActivityAddress(AddressListActivity.this,null,keyword);
      }
    }
  }
.execute();
}
