{
  if (comparison != 0) {
    return this;
  }
  if (lhs == rhs) {
    return this;
  }
  if (lhs == null) {
    comparison=-1;
    return this;
  }
  if (rhs == null) {
    comparison=+1;
    return this;
  }
  if (lhs.length != rhs.length) {
    comparison=(lhs.length < rhs.length) ? -1 : +1;
    return this;
  }
  for (int i=0; i < lhs.length && comparison == 0; i++) {
    append(lhs[i],rhs[i]);
  }
  return this;
}
