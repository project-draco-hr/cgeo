{
  if (progress.isShowing()) {
    showToast(res.getString(R.string.err_detail_still_working));
    return;
  }
  if (Settings.getChooseList()) {
    new StoredList.UserInterface(getActivity()).promptForListSelection(R.string.list_title,new Action1<Integer>(){
      @Override public void call(      final Integer selectedListId){
        storeCache(selectedListId);
      }
    }
,true,cache.getLists());
  }
 else {
    storeCache(StoredList.TEMPORARY_LIST.id);
  }
}
