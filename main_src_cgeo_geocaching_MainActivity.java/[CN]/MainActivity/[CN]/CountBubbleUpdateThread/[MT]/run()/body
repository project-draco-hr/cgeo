{
  if (app == null) {
    return;
  }
  int checks=0;
  while (!DataStore.isInitialized()) {
    try {
      wait(500);
      checks++;
    }
 catch (    Exception e) {
      Log.e("MainActivity.CountBubbleUpdateThread.run",e);
    }
    if (checks > 10) {
      return;
    }
  }
  countBubbleCnt=DataStore.getAllCachesCount();
  countBubbleHandler.sendEmptyMessage(0);
}
