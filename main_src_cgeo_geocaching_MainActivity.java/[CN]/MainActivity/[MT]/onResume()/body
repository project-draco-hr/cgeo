{
  super.onResume(locationUpdater.start(GeoDirHandler.UPDATE_GEODATA | GeoDirHandler.LOW_POWER),app.gpsStatusObservable().observeOn(AndroidSchedulers.mainThread()).subscribe(satellitesHandler));
  updateUserInfoHandler.sendEmptyMessage(-1);
  if (app.hasValidLocation()) {
    locationUpdater.updateGeoData(app.currentGeo());
  }
  startBackgroundLogin();
  init();
}
