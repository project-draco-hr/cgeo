{
  super(authParams.getSite(),"/okapi/services/oauth/request_token","/okapi/services/oauth/authorize","/okapi/services/oauth/access_token",false,cgeoapplication.getInstance().getResources().getString(authParams.getCKResId()),cgeoapplication.getInstance().getResources().getString(authParams.getCSResId()));
  this.authParams=authParams;
}
