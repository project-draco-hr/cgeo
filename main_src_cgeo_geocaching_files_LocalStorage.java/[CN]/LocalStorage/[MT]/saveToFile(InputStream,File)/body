{
  if (inputStream == null) {
    return false;
  }
  try {
    try {
      final FileOutputStream fos=new FileOutputStream(targetFile);
      try {
        return copy(inputStream,fos);
      }
  finally {
        fos.close();
      }
    }
  finally {
      inputStream.close();
    }
  }
 catch (  IOException e) {
    Log.e(Settings.tag,"LocalStorage.saveToFile",e);
  }
  return false;
}
