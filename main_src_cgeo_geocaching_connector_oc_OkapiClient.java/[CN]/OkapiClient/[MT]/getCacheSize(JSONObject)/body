{
  if (response.isNull(CACHE_SIZE2)) {
    return getCacheSizeDeprecated(response);
  }
  try {
    final String size=response.getString(CACHE_SIZE2);
    return CacheSize.getById(size);
  }
 catch (  JSONException e) {
    Log.e("OkapiClient.getCacheSize",e);
    return getCacheSizeDeprecated(response);
  }
}
