{
  if (StringUtils.isBlank(page)) {
    return false;
  }
  if (StringUtils.isBlank(guid)) {
    return false;
  }
  final Boolean found=Pattern.compile(guid,Pattern.CASE_INSENSITIVE).matcher(page).find();
  Log.i("cgCache.isGuidContainedInPage: guid '" + guid + "' "+ (found ? "" : "not ")+ "found");
  return found;
}
