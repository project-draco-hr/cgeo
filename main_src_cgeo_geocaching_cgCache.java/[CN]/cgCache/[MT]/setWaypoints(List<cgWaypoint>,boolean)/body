{
  this.waypoints.set(waypoints);
  finalDefined=false;
  if (waypoints != null) {
    for (    cgWaypoint waypoint : waypoints) {
      waypoint.setGeocode(geocode);
      if (waypoint.isFinalWithCoords()) {
        finalDefined=true;
      }
    }
  }
  return saveToDatabase && cgeoapplication.getInstance().saveWaypoints(this);
}
