{
  this.waypoints=waypoints;
  finalDefined=false;
  if (waypoints != null) {
    for (    cgWaypoint waypoint : waypoints) {
      waypoint.setGeocode(geocode);
      if (waypoint.isFinalWithCoords()) {
        finalDefined=true;
      }
    }
  }
  if (saveToDatabase) {
    return cgeoapplication.getInstance().saveWaypoints(geocode,waypoints,false);
  }
  return false;
}
