{
  if (!isEventCache()) {
    return false;
  }
  if (hidden == null) {
    return false;
  }
  final Calendar cal=Calendar.getInstance();
  cal.setTime(new Date());
  cal.set(Calendar.HOUR,0);
  cal.set(Calendar.MINUTE,0);
  cal.set(Calendar.SECOND,0);
  cal.set(Calendar.MILLISECOND,0);
  if (hidden.compareTo(cal.getTime()) < 0) {
    return false;
  }
  return true;
}
