{
  menu.add(0,MENU_ID_DEFAULT_NAVIGATION,0,res.getString(R.string.cache_menu_default_navigation)).setIcon(android.R.drawable.ic_menu_compass);
  SubMenu subMenu=menu.addSubMenu(1,MENU_ID_NAVIGATION,0,res.getString(R.string.cache_menu_navigate)).setIcon(android.R.drawable.ic_menu_mapmode);
  addNavigationMenuItems(subMenu);
  menu.add(0,MENU_ID_CACHES_AROUND,0,res.getString(R.string.cache_menu_around)).setIcon(android.R.drawable.ic_menu_rotate);
  menu.add(0,MENU_ID_OPEN_GEOCACHE,0,res.getString(R.string.waypoint_menu_open_cache)).setIcon(android.R.drawable.ic_menu_mylocation);
  return true;
}
