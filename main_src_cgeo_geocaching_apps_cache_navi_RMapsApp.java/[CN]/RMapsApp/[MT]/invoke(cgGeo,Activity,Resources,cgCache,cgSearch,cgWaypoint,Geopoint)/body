{
  if (cache == null && waypoint == null && coords == null) {
    return false;
  }
  try {
    if (isInstalled(activity)) {
      final ArrayList<String> locations=new ArrayList<String>();
      if (cache != null && cache.coords != null) {
        locations.add(String.format((Locale)null,"%.6f",cache.coords.getLatitude()) + "," + String.format((Locale)null,"%.6f",cache.coords.getLongitude())+ ";"+ cache.geocode+ ";"+ cache.name);
      }
 else       if (waypoint != null && waypoint.coords != null) {
        locations.add(String.format((Locale)null,"%.6f",waypoint.coords.getLatitude()) + "," + String.format((Locale)null,"%.6f",waypoint.coords.getLongitude())+ ";"+ waypoint.lookup+ ";"+ waypoint.name);
      }
      final Intent intent=new Intent(INTENT);
      intent.putStringArrayListExtra("locations",locations);
      activity.startActivity(intent);
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
