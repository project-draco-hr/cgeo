{
  NavigationApp app=(NavigationApp)getAppFromMenuItem(item,apps);
  if (app != null) {
    try {
      return app.invoke(geo,activity,res,cache,search,waypoint,destination);
    }
 catch (    Exception e) {
      Log.e(Settings.tag,"NavigationAppFactory.onMenuItemSelected: " + e.toString());
    }
  }
  return false;
}
