{
  if (ArrayUtils.isEmpty(apps)) {
    apps=new NavigationApp[]{new CompassApp(res),new RadarApp(res),new InternalMap(res),new StaticMapApp(res),new LocusApp(res),new RMapsApp(res),new GoogleMapsApp(res),new GoogleNavigationApp(res),new StreetviewApp(res),new OruxMapsApp(res),new NavigonApp()};
  }
  return apps;
}
