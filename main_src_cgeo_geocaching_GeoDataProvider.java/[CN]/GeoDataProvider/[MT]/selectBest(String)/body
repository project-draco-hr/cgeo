{
  if (locNet != null && locGps == null) {
    assign(locNet);
  }
 else   if ((locNet == null && locGps != null) || (current.satellitesFixed > 0) || (signallingProvider != null && signallingProvider.equals(LocationManager.GPS_PROVIDER))|| locGpsLast > System.currentTimeMillis() - 30 * 1000) {
    assign(locGps);
  }
 else {
    assign(locNet);
  }
  notifyObservers(current.makeImmutable());
}
