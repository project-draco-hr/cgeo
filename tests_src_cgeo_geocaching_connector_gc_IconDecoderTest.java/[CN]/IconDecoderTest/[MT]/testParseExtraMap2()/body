{
  final Bitmap bitmap=getBitmap(R.raw.map2);
  assertThat(parseMapPNG(bitmap,132,136,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,68,24,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,176,232,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,148,60,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,216,20,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,212,184,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,148,72,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,216,48,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,212,60,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,148,100,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,104,136,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,52,96,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,172,156,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,116,56,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,208,68,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,200,52,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,208,44,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,112,76,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,232,192,12).isOwner()).isTrue();
  assertThat(parseMapPNG(bitmap,184,76,12).isFound()).isTrue();
  assertThat(parseMapPNG(bitmap,132,148,12).isOwner()).isTrue();
}
