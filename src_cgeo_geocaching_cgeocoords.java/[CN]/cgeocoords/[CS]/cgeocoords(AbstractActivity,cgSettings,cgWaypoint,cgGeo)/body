{
  super(contextIn);
  context=contextIn;
  settings=settingsIn;
  geo=geoIn;
  if (waypoint != null) {
    latitude=waypoint.latitude;
    longitude=waypoint.longitude;
  }
 else   if (geo != null && geo.latitudeNow != null && geo.longitudeNow != null) {
    latitude=geo.latitudeNow;
    longitude=geo.longitudeNow;
  }
}
