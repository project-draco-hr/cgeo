{
  for (  final NameValuePair header : Parameters.extend(Parameters.merge(headers,cacheHeaders(cacheFile)),"Accept-Charset","utf-8,iso-8859-1;q=0.8,utf-16;q=0.8,*;q=0.7","Accept-Language","en-US,*;q=0.9","X-Requested-With","XMLHttpRequest")) {
    request.setHeader(header.getName(),header.getValue());
  }
  if (Settings.getUseNativeUa()) {
    request.getParams().setParameter(CoreProtocolPNames.USER_AGENT,Network.NATIVE_USER_AGENT);
  }
 else {
    request.getParams().setParameter(CoreProtocolPNames.USER_AGENT,Network.PC_USER_AGENT);
  }
}
