{
  try {
    return ((HttpURLConnection)new URL(url).openConnection()).getLastModified();
  }
 catch (  MalformedURLException e) {
    Log.e(Settings.tag,"Failed to get last modified date for: " + url + " "+ e);
  }
catch (  IOException e) {
    Log.e(Settings.tag,"Failed to get last modified date for: " + url + " "+ e);
  }
  return 0;
}
