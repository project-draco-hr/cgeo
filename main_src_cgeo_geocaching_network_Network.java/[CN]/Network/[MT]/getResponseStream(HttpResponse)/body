{
  if (!isSuccess(response)) {
    return null;
  }
  assert(response != null);
  final HttpEntity entity=response.getEntity();
  if (entity == null) {
    return null;
  }
  try {
    return entity.getContent();
  }
 catch (  final IOException e) {
    Log.e("Network.getResponseStream",e);
    return null;
  }
}
