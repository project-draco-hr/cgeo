{
  if (input == null) {
    return null;
  }
  input=input.trim();
  try {
    Date result;
    if (input.indexOf('/') > 0) {
      result=dateInSlash.parse(input);
      if (result != null) {
        return result;
      }
    }
    if (input.indexOf('-') > 0) {
      result=dateInDash.parse(input);
      if (result != null) {
        return result;
      }
    }
    result=dateEvIn.parse(input);
    if (result != null) {
      return result;
    }
  }
 catch (  ParseException e) {
    e.printStackTrace();
  }
  return null;
}
