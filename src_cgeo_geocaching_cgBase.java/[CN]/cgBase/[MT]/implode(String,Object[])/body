{
  StringBuilder out=new StringBuilder();
  try {
    for (int i=0; i < array.length; i++) {
      if (i != 0) {
        out.append(delim);
      }
      out.append(array[i].toString());
    }
  }
 catch (  Exception e) {
    Log.e(cgSettings.tag,"cgeoBase.implode: " + e.toString());
  }
  return out.toString();
}
