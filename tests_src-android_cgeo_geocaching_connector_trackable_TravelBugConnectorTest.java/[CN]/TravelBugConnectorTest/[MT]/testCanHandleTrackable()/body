{
  assertThat(getConnector().canHandleTrackable("TB1234")).isTrue();
  assertThat(getConnector().canHandleTrackable("TB1")).isTrue();
  assertThat(getConnector().canHandleTrackable("TB123F")).isTrue();
  assertThat(getConnector().canHandleTrackable("TB123Z")).isTrue();
  assertThat(getConnector().canHandleTrackable("TB4JD36")).isTrue();
  assertThat(getConnector().canHandleTrackable("GK1234")).isTrue();
  assertThat(getConnector().canHandleTrackable("GST9HV")).isTrue();
  assertThat(getConnector().canHandleTrackable("UNKNOWN")).isFalse();
  assertThat(getConnector().canHandleTrackable("GC1234")).isTrue();
  assertThat(getConnector().canHandleTrackable("GC1234",TrackableBrand.UNKNOWN)).isTrue();
  assertThat(getConnector().canHandleTrackable("GC1234",TrackableBrand.TRAVELBUG)).isTrue();
  assertThat(getConnector().canHandleTrackable("GC1234",TrackableBrand.GEOKRETY)).isFalse();
}
