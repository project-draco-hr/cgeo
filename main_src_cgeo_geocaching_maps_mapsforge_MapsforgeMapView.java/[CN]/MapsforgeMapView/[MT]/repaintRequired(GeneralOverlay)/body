{
  if (null == overlay) {
    invalidate();
  }
 else {
    try {
      Overlay ovl=(Overlay)overlay.getOverlayImpl();
      if (ovl != null) {
        ovl.requestRedraw();
      }
    }
 catch (    Exception e) {
      Log.e(Settings.tag,"MapsforgeMapView.repaintRequired: " + e.toString());
    }
  }
}
