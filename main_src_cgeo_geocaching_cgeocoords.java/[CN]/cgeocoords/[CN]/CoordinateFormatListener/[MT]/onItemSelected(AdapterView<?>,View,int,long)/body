{
  if (currentFormat != null) {
    if (!calc(false)) {
      if (geo != null && geo.getCoords() != null) {
        gp=geo.getCoords();
      }
 else {
        gp=new Geopoint(0.0,0.0);
      }
    }
  }
  currentFormat=coordInputFormatEnum.fromInt(pos);
  Settings.setCoordInputFormat(currentFormat);
  updateGUI();
}
