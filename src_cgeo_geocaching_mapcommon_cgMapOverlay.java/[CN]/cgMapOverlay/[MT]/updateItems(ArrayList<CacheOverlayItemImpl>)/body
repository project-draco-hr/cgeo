{
  if (itemsPre == null) {
    return;
  }
  for (  CacheOverlayItemImpl item : itemsPre) {
    item.setMarker(boundCenterBottom(item.getMarker(0)));
  }
  getOverlayImpl().lock();
  try {
    items=new ArrayList<CacheOverlayItemImpl>(itemsPre);
    setLastFocusedItemIndex(-1);
    populate();
  }
  finally {
    getOverlayImpl().unlock();
  }
}
