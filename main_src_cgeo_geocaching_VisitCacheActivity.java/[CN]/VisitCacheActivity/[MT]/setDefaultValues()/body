{
  date=Calendar.getInstance();
  rating=0.0;
  if (cache.isEventCache()) {
    if (cache.hasOwnLog(LogType.WILL_ATTEND)) {
      typeSelected=LogType.ATTENDED;
    }
 else {
      typeSelected=LogType.WILL_ATTEND;
    }
  }
 else {
    if (cache.isFound()) {
      typeSelected=LogType.NOTE;
    }
 else {
      typeSelected=LogType.FOUND_IT;
    }
  }
  text=null;
  imageCaption="";
  imageDescription="";
  imageUri=Uri.EMPTY;
}
