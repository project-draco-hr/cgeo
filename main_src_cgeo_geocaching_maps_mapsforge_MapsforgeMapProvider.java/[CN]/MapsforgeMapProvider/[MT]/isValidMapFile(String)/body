{
  if (StringUtils.isEmpty(mapFileIn)) {
    return false;
  }
  MapDatabase mapDB=new MapDatabase();
  FileOpenResult result=mapDB.openFile(new File(mapFileIn));
  mapDB.closeFile();
  boolean isValid=result.isSuccess();
  if (!isValid) {
    isValid=isMapfile024(mapFileIn);
  }
  return isValid;
}
