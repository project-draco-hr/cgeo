{
  init();
  database.beginTransaction();
  try {
    database.delete(dbTableSearchDestionationHistory,null,null);
    database.setTransactionSuccessful();
    return true;
  }
 catch (  Exception e) {
    Log.e("Unable to clear searched destinations",e);
  }
 finally {
    database.endTransaction();
  }
  return false;
}
