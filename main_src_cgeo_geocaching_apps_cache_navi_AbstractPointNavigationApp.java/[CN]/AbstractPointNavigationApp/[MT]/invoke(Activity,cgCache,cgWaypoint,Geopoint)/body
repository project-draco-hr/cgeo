{
  if (cache == null && waypoint == null && coords == null) {
    return false;
  }
  try {
    if (isInstalled()) {
      final Geopoint point=getCoordinates(cache,waypoint,coords);
      if (point != null) {
        navigate(activity,point);
        return true;
      }
    }
  }
 catch (  Exception e) {
  }
  return false;
}
