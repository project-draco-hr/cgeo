{
  if (id < 0) {
    return;
  }
  StoredList list=app.getList(id);
  if (list == null) {
    return;
  }
  listId=list.id;
  title=list.title;
  Settings.saveLastList(listId);
  showProgress(true);
  showFooterLoadingCaches();
  (new MoveCachesToListThread(listId,new MoveHandler())).start();
  invalidateOptionsMenuCompatible();
}
