{
  StoredList list=null;
  if (id >= 0) {
    list=app.getList(id);
  }
 else {
    return;
  }
  if (list == null) {
    return;
  }
  listId=list.id;
  title=list.title;
  Settings.saveLastList(listId);
  showProgress(true);
  setLoadingCaches();
  (new MoveCachesToListThread(listId,new MoveHandler())).start();
  invalidateOptionsMenuCompatible();
}
