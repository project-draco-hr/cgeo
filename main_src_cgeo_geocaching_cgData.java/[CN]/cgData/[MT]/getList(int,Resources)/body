{
  if (id == 1) {
    return new cgList(true,1,res.getString(R.string.list_inbox));
  }
 else   if (id == 2) {
    return new cgList(true,2,res.getString(R.string.list_wpt));
  }
 else   if (id >= 10) {
    init();
    ArrayList<cgList> lists=readLists("_id = " + (id - 10),null);
    if (!lists.isEmpty()) {
      return lists.get(0);
    }
  }
  return null;
}
