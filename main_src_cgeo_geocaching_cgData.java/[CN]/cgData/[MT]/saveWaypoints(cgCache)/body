{
  boolean result=false;
  init();
  database.beginTransaction();
  try {
    saveOriginalWaypointsWithoutTransaction(cache);
    database.setTransactionSuccessful();
    result=true;
  }
 catch (  Exception e) {
    Log.e("saveWaypoints",e);
  }
 finally {
    database.endTransaction();
  }
  return result;
}
