{
  if (StringUtils.isBlank(geocode)) {
    return null;
  }
  init();
  final Cursor cursor=database.query(dbTableLogsOffline,new String[]{"_id","type","log","date"},"geocode = ?",new String[]{geocode},null,null,"_id desc","1");
  LogEntry log=null;
  if (cursor.moveToFirst()) {
    log=new LogEntry(cursor.getLong(3),LogType.getById(cursor.getInt(1)),cursor.getString(2));
    log.id=cursor.getInt(0);
  }
  cursor.close();
  return log;
}
