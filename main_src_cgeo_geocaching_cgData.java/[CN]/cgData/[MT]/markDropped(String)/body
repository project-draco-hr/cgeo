{
  if (StringUtils.isBlank(geocode)) {
    return false;
  }
  init();
  try {
    ContentValues values=new ContentValues();
    values.put("reason",0);
    int rows=databaseRW.update(dbTableCaches,values,"geocode = ?",new String[]{geocode});
    if (rows > 0) {
      return true;
    }
  }
 catch (  Exception e) {
    Log.e(Settings.tag,"cgData.markDropped: " + e.toString());
  }
  return false;
}
