{
  if (id == 0) {
    return null;
  }
  init();
  Cursor cursor=database.query(dbTableWaypoints,WAYPOINT_COLUMNS,"_id = ?",new String[]{Integer.toString(id)},null,null,null,"1");
  Log.d("cgData.loadWaypoint(" + id + ")");
  cgWaypoint waypoint=null;
  if (cursor != null && cursor.getCount() > 0) {
    cursor.moveToFirst();
    waypoint=createWaypointFromDatabaseContent(cursor);
  }
  if (cursor != null) {
    cursor.close();
  }
  return waypoint;
}
