{
  if (id == 0) {
    return null;
  }
  init();
  final Cursor cursor=database.query(dbTableWaypoints,WAYPOINT_COLUMNS,"_id = ?",new String[]{Integer.toString(id)},null,null,null,"1");
  Log.d("cgData.loadWaypoint(" + id + ")");
  final cgWaypoint waypoint=cursor.moveToFirst() ? createWaypointFromDatabaseContent(cursor) : null;
  cursor.close();
  return waypoint;
}
