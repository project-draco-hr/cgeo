{
  if (centerLat == null || centerLon == null || spanLat == null || spanLon == null) {
    return null;
  }
  init();
  Set<String> geocodes=new HashSet<String>();
  StringBuilder where=buildCoordinateWhere(centerLat,centerLon,spanLat,spanLon);
  if (cacheType != CacheType.ALL) {
    where.append(" and type = \"");
    where.append(cacheType.id);
    where.append('"');
  }
  if (stored) {
    where.append(" and reason >= 1");
  }
  try {
    Cursor cursor=databaseRO.query(dbTableCaches,new String[]{"geocode"},where.toString(),null,null,null,null,"500");
    if (cursor != null) {
      if (cursor.getCount() > 0) {
        cursor.moveToFirst();
        int index=cursor.getColumnIndex("geocode");
        do {
          geocodes.add(cursor.getString(index));
        }
 while (cursor.moveToNext());
      }
 else {
        cursor.close();
        return null;
      }
      cursor.close();
    }
  }
 catch (  Exception e) {
    Log.e(Settings.tag,"cgData.loadInViewport: " + e.toString());
  }
  return geocodes;
}
