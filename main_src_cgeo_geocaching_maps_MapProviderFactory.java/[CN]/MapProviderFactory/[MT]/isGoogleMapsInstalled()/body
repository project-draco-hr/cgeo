{
  if (StringUtils.isBlank(CgeoApplication.getInstance().getString(R.string.maps_api_key))) {
    Log.w("No Google API key available.");
    return false;
  }
  try {
    Class.forName("com.google.android.maps.MapActivity");
  }
 catch (  ClassNotFoundException e) {
    return false;
  }
  return true;
}
