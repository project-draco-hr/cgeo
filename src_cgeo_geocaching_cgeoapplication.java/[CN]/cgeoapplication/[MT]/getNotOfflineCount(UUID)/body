{
  if (searchId == null || searches.containsKey(searchId) == false) {
    return 0;
  }
  int count=0;
  List<String> geocodes=searches.get(searchId).getGeocodes();
  if (geocodes != null) {
    for (    String geocode : geocodes) {
      if (isOffline(geocode,null) == false) {
        count++;
      }
    }
  }
  return count;
}
