{
  int check=0;
  for (  cgCache cache : list) {
    if (cache.isStatusChecked()) {
      cache.setStatusChecked(false);
      cache.setStatusCheckedView(false);
    }
 else {
      cache.setStatusChecked(true);
      cache.setStatusCheckedView(true);
      check++;
    }
  }
  checkChecked(check);
  notifyDataSetChanged();
}
