{
  int check=0;
  for (  cgCache cache : list) {
    if (cache.statusChecked) {
      cache.statusChecked=false;
      cache.statusCheckedView=false;
    }
 else {
      cache.statusChecked=true;
      cache.statusCheckedView=true;
      check++;
    }
  }
  checkChecked(check);
  notifyDataSetChanged();
}
