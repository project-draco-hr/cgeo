{
  selectMode=status;
  if (selectMode == false && clear) {
    for (    cgCache cache : list) {
      cache.statusChecked=false;
      cache.statusCheckedView=false;
    }
    checked=0;
  }
 else   if (selectMode) {
    for (    cgCache cache : list) {
      cache.statusCheckedView=false;
    }
  }
  checkChecked(0);
  notifyDataSetChanged();
  return selectMode;
}
