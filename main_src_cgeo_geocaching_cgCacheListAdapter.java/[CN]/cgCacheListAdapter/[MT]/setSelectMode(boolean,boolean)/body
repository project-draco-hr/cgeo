{
  selectMode=status;
  if (!selectMode && clear) {
    for (    final cgCache cache : list) {
      cache.statusChecked=false;
      cache.statusCheckedView=false;
    }
    checked=0;
  }
 else   if (selectMode) {
    for (    final cgCache cache : list) {
      cache.statusCheckedView=false;
    }
  }
  checkChecked(0);
  notifyDataSetChanged();
  return selectMode;
}
