{
  selectMode=status;
  if (!selectMode && clear) {
    for (    final cgCache cache : list) {
      cache.setStatusChecked(false);
      cache.setStatusCheckedView(false);
    }
    checked=0;
  }
 else   if (selectMode) {
    for (    final cgCache cache : list) {
      cache.setStatusCheckedView(false);
    }
  }
  checkChecked(0);
  notifyDataSetChanged();
  return selectMode;
}
