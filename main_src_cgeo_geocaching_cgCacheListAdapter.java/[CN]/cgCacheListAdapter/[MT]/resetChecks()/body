{
  if (list.isEmpty()) {
    return false;
  }
  if (checked <= 0) {
    return false;
  }
  boolean status=getSelectMode();
  int cleared=0;
  for (  cgCache cache : list) {
    if (cache.statusChecked) {
      cache.statusChecked=false;
      checkChecked(-1);
      cleared++;
    }
  }
  setSelectMode(false,false);
  notifyDataSetChanged();
  if (cleared > 0 || status) {
    return true;
  }
 else {
    return false;
  }
}
