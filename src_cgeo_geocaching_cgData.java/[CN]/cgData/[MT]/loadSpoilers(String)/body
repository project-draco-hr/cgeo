{
  if (StringUtils.isBlank(geocode)) {
    return null;
  }
  init();
  List<cgImage> spoilers=new ArrayList<cgImage>();
  Cursor cursor=databaseRO.query(dbTableSpoilers,new String[]{"_id","url","title","description"},"geocode = \"" + geocode + "\"",null,null,null,null,"100");
  if (cursor != null && cursor.getCount() > 0) {
    cursor.moveToFirst();
    do {
      cgImage spoiler=new cgImage();
      spoiler.url=(String)cursor.getString(cursor.getColumnIndex("url"));
      spoiler.title=(String)cursor.getString(cursor.getColumnIndex("title"));
      spoiler.description=(String)cursor.getString(cursor.getColumnIndex("description"));
      spoilers.add(spoiler);
    }
 while (cursor.moveToNext());
  }
  if (cursor != null) {
    cursor.close();
  }
  return spoilers;
}
