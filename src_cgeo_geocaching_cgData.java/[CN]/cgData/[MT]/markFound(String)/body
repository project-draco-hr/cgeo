{
  if (geocode == null || geocode.length() == 0) {
    return false;
  }
  init();
  try {
    ContentValues values=new ContentValues();
    values.put("found",1);
    int rows=databaseRW.update(dbTableCaches,values,"geocode = ?",new String[]{geocode});
    if (rows > 0) {
      return true;
    }
  }
 catch (  Exception e) {
    Log.e(cgSettings.tag,"cgData.markFound: " + e.toString());
  }
  return false;
}
