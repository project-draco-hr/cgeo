{
  if (StringUtils.isBlank(guid)) {
    return null;
  }
  init();
  Cursor cursor=null;
  String geocode=null;
  try {
    cursor=databaseRO.query(dbTableCaches,new String[]{"_id","geocode"},"guid = \"" + guid + "\"",null,null,null,null,"1");
    if (cursor != null) {
      int index=0;
      if (cursor.getCount() > 0) {
        cursor.moveToFirst();
        index=cursor.getColumnIndex("geocode");
        geocode=(String)cursor.getString(index);
      }
    }
  }
 catch (  Exception e) {
    Log.e(cgSettings.tag,"cgData.getGeocodeForGuid: " + e.toString());
  }
  if (cursor != null) {
    cursor.close();
  }
  return geocode;
}
