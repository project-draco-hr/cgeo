{
  init();
  List<String> geocodes=new ArrayList<String>();
  StringBuilder where=new StringBuilder();
  if (cachetype != null) {
    where.append(cachetype);
    where.append('"');
  }
  if (where.length() > 0) {
    where.append(" and ");
  }
  where.append("reason >= 1");
  try {
    Cursor cursor=databaseRO.query(dbTableCaches,new String[]{"_id","geocode"},where.toString(),null,null,null,null,"5000");
    if (cursor != null) {
      if (cursor.getCount() > 0) {
        cursor.moveToFirst();
        do {
          geocodes.add((String)cursor.getString(cursor.getColumnIndex("geocode")));
        }
 while (cursor.moveToNext());
      }
 else {
        cursor.close();
        return null;
      }
      cursor.close();
    }
  }
 catch (  Exception e) {
    Log.e(cgSettings.tag,"cgData.getOfflineAll: " + e.toString());
  }
  return geocodes;
}
