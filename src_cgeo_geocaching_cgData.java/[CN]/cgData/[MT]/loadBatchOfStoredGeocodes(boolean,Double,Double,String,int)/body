{
  init();
  if (list < 1) {
    list=1;
  }
  Cursor cursor=null;
  ArrayList<String> geocodes=new ArrayList<String>();
  StringBuilder specifySql=new StringBuilder();
  specifySql.append("reason = ");
  specifySql.append(list);
  if (detailedOnly) {
    if (specifySql.length() > 0) {
      specifySql.append(" and ");
    }
    specifySql.append("detailed = 1");
  }
  if (cachetype != null) {
    if (specifySql.length() > 0) {
      specifySql.append(" and ");
    }
    specifySql.append("type = \"");
    specifySql.append(cachetype);
    specifySql.append("\"");
  }
  try {
    cursor=databaseRO.query(dbTableCaches,new String[]{"_id","geocode","(abs(latitude-" + String.format((Locale)null,"%.6f",latitude) + ") + abs(longitude-"+ String.format((Locale)null,"%.6f",longitude)+ ")) as dif"},specifySql.toString(),null,null,null,"dif",null);
    if (cursor != null) {
      if (cursor.getCount() > 0) {
        cursor.moveToFirst();
        do {
          geocodes.add((String)cursor.getString(cursor.getColumnIndex("geocode")));
        }
 while (cursor.moveToNext());
      }
 else {
        cursor.close();
        return null;
      }
      cursor.close();
    }
  }
 catch (  Exception e) {
    Log.e(cgSettings.tag,"cgData.loadBatchOfStoredGeocodes: " + e.toString());
  }
  return geocodes;
}
