{
  if (wp_id <= 0) {
    return;
  }
  for (int level=1; level <= 5; level++) {
    try {
      StaticMapsProvider.getMapFile(geocode,WAYPOINT_PREFIX + wp_id + '_'+ level,false).delete();
    }
 catch (    Exception e) {
      Log.e("StaticMapsProvider.removeWpStaticMaps: " + e.toString());
    }
  }
}
