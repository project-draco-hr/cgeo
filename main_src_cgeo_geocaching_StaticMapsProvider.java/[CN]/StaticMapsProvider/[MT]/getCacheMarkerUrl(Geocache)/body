{
  StringBuilder url=new StringBuilder(MARKERS_URL);
  url.append("marker_cache_").append(cache.getType().id);
  if (cache.isFound()) {
    url.append("_found");
  }
 else   if (cache.isDisabled() || cache.isArchived()) {
    url.append("_disabled");
  }
  url.append(".png");
  return url.toString();
}
