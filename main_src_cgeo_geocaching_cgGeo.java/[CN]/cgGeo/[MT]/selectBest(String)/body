{
  if (locNet != null && locGps == null) {
    assign(locNet);
  }
 else   if ((locNet == null && locGps != null) || (satellitesFixed > 0) || (signallingProvider != null && signallingProvider.equals(LocationManager.GPS_PROVIDER))|| locGpsLast > (System.currentTimeMillis() - 30 * 1000)) {
    assign(locGps);
  }
 else {
    assign(locNet);
  }
}
