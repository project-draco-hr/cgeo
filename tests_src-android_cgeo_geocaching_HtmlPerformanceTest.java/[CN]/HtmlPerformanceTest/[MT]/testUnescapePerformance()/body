{
  final int runs=100;
  measure("unescape Apache",new Runnable(){
    @Override public void run(){
      for (int i=0; i < runs; i++) {
        unescapeApache();
      }
    }
  }
);
  measure("unescape Android",new Runnable(){
    @Override public void run(){
      for (int i=0; i < runs; i++) {
        unescapeAndroid();
      }
    }
  }
);
}
