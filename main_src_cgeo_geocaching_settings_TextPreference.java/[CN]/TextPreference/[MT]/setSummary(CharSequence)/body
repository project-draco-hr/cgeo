{
  if (this.summaryView == null) {
    this.summaryText=summaryText;
    return;
  }
  if (summaryText == null) {
    if (this.summaryText == null) {
      return;
    }
    this.summaryView.setText(this.summaryText);
  }
 else {
    this.summaryView.setText(summaryText);
  }
  this.summaryView.setVisibility(View.VISIBLE);
}
