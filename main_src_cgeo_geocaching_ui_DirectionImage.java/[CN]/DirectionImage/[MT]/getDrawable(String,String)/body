{
  if (StringUtils.isBlank(geocode) || StringUtils.isBlank(code)) {
    return;
  }
  final HttpResponse httpResponse=Network.request("http://www.geocaching.com/ImgGen/seek/CacheDir.ashx",new Parameters("k",code),false);
  if (httpResponse != null) {
    LocalStorage.saveEntityToFile(httpResponse,getDirectionFile(geocode,true));
  }
}
