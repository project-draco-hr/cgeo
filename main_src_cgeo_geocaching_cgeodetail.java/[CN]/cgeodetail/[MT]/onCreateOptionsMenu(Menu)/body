{
  if (null != cache) {
    menu.add(0,MENU_NAVIGATE,0,res.getString(R.string.cache_menu_compass)).setIcon(android.R.drawable.ic_menu_compass);
    final SubMenu subMenu=menu.addSubMenu(1,0,0,res.getString(R.string.cache_menu_navigate)).setIcon(android.R.drawable.ic_menu_mapmode);
    addNavigationMenuItems(subMenu);
    menu.add(1,MENU_CALENDAR,0,res.getString(R.string.cache_menu_event)).setIcon(android.R.drawable.ic_menu_agenda);
    addVisitMenu(menu,cache);
    menu.add(1,MENU_SPOILERS,0,res.getString(R.string.cache_menu_spoilers)).setIcon(android.R.drawable.ic_menu_gallery);
    menu.add(0,MENU_CACHES_AROUND,0,res.getString(R.string.cache_menu_around)).setIcon(android.R.drawable.ic_menu_rotate);
    menu.add(1,MENU_BROWSER,0,res.getString(R.string.cache_menu_browser)).setIcon(R.drawable.ic_menu_globe);
    menu.add(0,MENU_SHARE,0,res.getString(R.string.cache_menu_share)).setIcon(android.R.drawable.ic_menu_share);
  }
  return true;
}
