{
  final LayerDrawable layerDrawable=new LayerDrawable(new Drawable[]{Compatibility.getDrawable(res,drawableResourceId),Compatibility.getDrawable(res,R.drawable.cgeo)});
  layerDrawable.setLayerInset(0,0,0,10,10);
  layerDrawable.setLayerInset(1,50,50,0,0);
  return ImageUtils.convertToBitmap(layerDrawable);
}
