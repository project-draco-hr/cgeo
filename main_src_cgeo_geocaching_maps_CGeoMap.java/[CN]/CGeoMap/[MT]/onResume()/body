{
  super.onResume();
  addGeoDirObservers();
  if (!CollectionUtils.isEmpty(dirtyCaches)) {
    for (    String geocode : dirtyCaches) {
      cgCache cache=cgData.loadCache(geocode,LoadFlags.LOAD_WAYPOINTS);
      caches.remove(cache);
      caches.add(cache);
    }
    dirtyCaches.clear();
    displayExecutor.execute(new DisplayRunnable(mapView.getViewport()));
  }
  startTimer();
}
