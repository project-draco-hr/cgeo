{
  if (loadTimer != null) {
    loadTimer.stopIt();
    loadTimer=null;
  }
  if (go4CacheTimer != null) {
    go4CacheTimer.stopIt();
    go4CacheTimer=null;
  }
  if (dir != null) {
    dir=app.removeDir();
  }
  app.deleteGeoObserver(geoUpdate);
  savePrefs();
  if (mapView != null) {
    mapView.destroyDrawingCache();
  }
  super.onPause();
}
