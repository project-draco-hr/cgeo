{
  if (loadTimer != null) {
    loadTimer.stopIt();
    loadTimer=null;
  }
  if (go4CacheTimer != null) {
    go4CacheTimer.stopIt();
    go4CacheTimer=null;
  }
  deleteGeoDirObservers();
  savePrefs();
  if (mapView != null) {
    mapView.destroyDrawingCache();
  }
  overlaysCache.clear();
  super.onPause();
}
