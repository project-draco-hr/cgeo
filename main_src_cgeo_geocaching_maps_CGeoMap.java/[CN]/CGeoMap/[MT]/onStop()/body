{
  if (loadTimer != null) {
    loadTimer.stopIt();
    loadTimer=null;
  }
  if (go4CacheTimer != null) {
    go4CacheTimer.stopIt();
    go4CacheTimer=null;
  }
  if (dir != null) {
    dir=app.removeDir();
  }
  if (geo != null) {
    geo=app.removeGeo();
  }
  savePrefs();
  if (mapView != null) {
    mapView.destroyDrawingCache();
  }
  super.onStop();
}
