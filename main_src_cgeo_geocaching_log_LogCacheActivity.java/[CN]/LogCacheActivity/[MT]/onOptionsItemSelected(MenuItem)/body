{
switch (item.getItemId()) {
case R.id.menu_send:
    sendLogAndConfirm();
  return true;
case R.id.menu_image:
selectImage();
return true;
case R.id.save:
saveLog(true);
finish();
return true;
case R.id.clear:
clearLog();
return true;
default :
break;
}
return super.onOptionsItemSelected(item);
}
