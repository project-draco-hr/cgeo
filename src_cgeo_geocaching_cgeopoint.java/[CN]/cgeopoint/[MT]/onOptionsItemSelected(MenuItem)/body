{
  final int menuItem=item.getItemId();
  ArrayList<Double> coords=getDestination();
  if (menuItem == 1) {
    showOnMap();
    return true;
  }
 else   if (menuItem == 2) {
    navigateTo();
    return true;
  }
 else   if (menuItem == 5) {
    cachesAround();
    return true;
  }
  return NavigationAppFactory.onMenuItemSelected(item,geo,activity,res,warning,tracker,null,null,null,coords);
}
