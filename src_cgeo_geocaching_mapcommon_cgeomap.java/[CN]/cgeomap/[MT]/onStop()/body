{
  if (loadTimer != null) {
    loadTimer.stopIt();
    loadTimer=null;
  }
  if (usersTimer != null) {
    usersTimer.stopIt();
    usersTimer=null;
  }
  if (dir != null) {
    dir=app.removeDir();
  }
  if (geo != null) {
    geo=app.removeGeo();
  }
  savePrefs();
  if (mapView != null) {
    mapView.destroyDrawingCache();
  }
  super.onStop();
}
