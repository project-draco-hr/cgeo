{
  final int menuItem=item.getItemId();
  if (menuItem == MENU_ID_COMPASS) {
    goCompass(null);
    return true;
  }
 else   if (menuItem == MENU_ID_CACHES_AROUND) {
    cachesAround();
    return true;
  }
  return NavigationAppFactory.onMenuItemSelected(item,geo,activity,res,warning,null,null,waypoint,null);
}
