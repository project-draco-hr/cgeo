{
  if (cache == null) {
    loadCookiesIfNeeded(prefs);
    final ArrayList<String> built=new ArrayList<String>();
    for (    final Map.Entry<String,String> entry : cookies.entrySet()) {
      built.add(entry.getKey() + "=" + entry.getValue());
    }
    cache=StringUtils.join(built,';');
  }
  return cache;
}
