{
  boolean statusChecked=false;
  boolean statusSort=false;
  checked+=cnt;
  if (checked > 0) {
    statusChecked=false;
  }
 else {
    statusChecked=true;
  }
  if (getSelectMode()) {
    statusSort=false;
  }
 else {
    statusSort=true;
  }
  if (statusChecked == false || statusSort == false) {
    sort=false;
  }
 else {
    sort=true;
  }
  if (sort) {
    forceSort(latitude,longitude);
  }
}
