{
  CacheListApp app=(CacheListApp)getAppFromMenuItem(item,apps);
  if (app != null) {
    try {
      return app.invoke(geo,caches,activity,res,searchId);
    }
 catch (    Exception e) {
      Log.e(cgSettings.tag,"CacheListAppFactory.onMenuItemSelected: " + e.toString());
    }
  }
  return false;
}
