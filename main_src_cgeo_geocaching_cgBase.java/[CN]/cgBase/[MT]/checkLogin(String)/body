{
  if (StringUtils.isBlank(page)) {
    Log.e(Settings.tag,"cgeoBase.checkLogin: No page given");
    return false;
  }
  if (BaseUtils.matches(page,GCConstants.PATTERN_LOGGEDIN2)) {
    return true;
  }
  if (BaseUtils.matches(page,GCConstants.PATTERN_LOGGEDIN)) {
    return true;
  }
  return false;
}
