{
  if (app == null) {
    return;
  }
  int checks=0;
  while (!app.storageStatus()) {
    try {
      wait(500);
      checks++;
    }
 catch (    Exception e) {
    }
    if (checks > 10) {
      return;
    }
  }
  countBubbleCnt=cgeoapplication.getAllCachesCount();
  countBubbleHandler.sendEmptyMessage(0);
}
