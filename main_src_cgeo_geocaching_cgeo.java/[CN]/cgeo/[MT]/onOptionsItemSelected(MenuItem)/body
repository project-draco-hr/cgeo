{
  final int id=item.getItemId();
switch (id) {
case MENU_ABOUT:
    showAbout(null);
  return true;
case MENU_HELPERS:
startActivity(new Intent(this,UsefulAppsActivity.class));
return true;
case MENU_SETTINGS:
startActivity(new Intent(this,SettingsActivity.class));
return true;
case MENU_HISTORY:
cgeocaches.startActivityHistory(this);
return true;
case MENU_SCAN:
startScannerApplication();
return true;
default :
break;
}
return false;
}
