{
  cgList list=null;
  if (id >= 0) {
    list=app.getList(id);
  }
 else   if (order >= 0) {
    lists=app.getLists();
    list=lists.get(order);
  }
 else {
    return;
  }
  if (list == null) {
    return;
  }
  listId=list.id;
  title=list.title;
  settings.saveLastList(listId);
  showProgress(true);
  setLoadingCaches();
  (new moveCachesToList(listId,new MoveHandler())).start();
}
