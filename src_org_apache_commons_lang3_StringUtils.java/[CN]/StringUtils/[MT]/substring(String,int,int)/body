{
  if (str == null) {
    return null;
  }
  if (end < 0) {
    end=str.length() + end;
  }
  if (start < 0) {
    start=str.length() + start;
  }
  if (end > str.length()) {
    end=str.length();
  }
  if (start > end) {
    return EMPTY;
  }
  if (start < 0) {
    start=0;
  }
  if (end < 0) {
    end=0;
  }
  return str.substring(start,end);
}
