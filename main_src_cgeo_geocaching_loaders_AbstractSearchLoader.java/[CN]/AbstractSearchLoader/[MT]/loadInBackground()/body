{
  loading=true;
  try {
    if (search == null) {
      search=runSearch();
    }
 else {
      search=new SearchResult(search);
    }
  }
 catch (  final Exception e) {
    Log.e("Error in Loader ",e);
  }
  loading=false;
  if (search == null) {
    search=new SearchResult();
  }
  return search;
}
