{
  loading=true;
  try {
    if (search == null) {
      search=runSearch();
    }
 else {
      search=GCParser.searchByNextPage(new SearchResult(search),Settings.isShowCaptcha(),this);
    }
  }
 catch (  Exception e) {
    Log.e("Error in Loader ",e);
  }
  loading=false;
  if (search == null) {
    search=new SearchResult();
  }
  return search;
}
