{
  File inFile=new File(args[0]);
  InputStream inputStream;
  try {
    writeHeader();
    inputStream=new FileInputStream(inFile);
    Reader reader=new InputStreamReader(inputStream,"UTF-8");
    InputSource is=new InputSource(reader);
    is.setEncoding("UTF-8");
    parseAttributes(is);
    writeTrailer();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
