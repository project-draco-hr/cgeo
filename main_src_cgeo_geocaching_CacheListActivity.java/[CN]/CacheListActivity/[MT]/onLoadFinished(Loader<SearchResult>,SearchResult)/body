{
  if (searchIn != null) {
    cacheList.clear();
    final Set<Geocache> cachesFromSearchResult=searchIn.getCachesFromSearchResult(LoadFlags.LOAD_CACHE_OR_DB);
    cacheList.addAll(cachesFromSearchResult);
    search=searchIn;
    adapter.reFilter();
    adapter.checkEvents();
    adapter.forceSort();
    adapter.notifyDataSetChanged();
    updateTitle();
    showFooterMoreCaches();
  }
  showProgress(false);
  hideLoading();
}
