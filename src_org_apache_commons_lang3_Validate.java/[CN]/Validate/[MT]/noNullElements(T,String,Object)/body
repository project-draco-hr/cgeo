{
  Validate.notNull(iterable);
  int i=0;
  for (Iterator<?> it=iterable.iterator(); it.hasNext(); i++) {
    if (it.next() == null) {
      Object[] values2=ArrayUtils.addAll(values,Integer.valueOf(i));
      throw new IllegalArgumentException(String.format(message,values2));
