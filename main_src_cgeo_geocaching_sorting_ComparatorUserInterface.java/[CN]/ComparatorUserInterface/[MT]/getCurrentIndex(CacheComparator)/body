{
  for (int index=0; index < registry.size(); index++) {
    final ComparatorEntry entry=registry.get(index);
    if (current == null) {
      if (entry.cacheComparator == null) {
        return index;
      }
    }
 else     if (current.getClass().equals(entry.cacheComparator)) {
      return index;
    }
  }
  return -1;
}
