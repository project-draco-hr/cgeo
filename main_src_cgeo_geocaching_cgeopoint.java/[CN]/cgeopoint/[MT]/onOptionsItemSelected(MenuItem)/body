{
  final int menuItem=item.getItemId();
  final Geopoint coords=getDestination();
  if (coords != null) {
    addToHistory(coords);
  }
switch (menuItem) {
case MENU_DEFAULT_NAVIGATION:
    navigateTo();
  return true;
case MENU_CACHES_AROUND:
cachesAround();
return true;
case MENU_CLEAR_HISTORY:
clearHistory();
return true;
default :
return NavigationAppFactory.onMenuItemSelected(item,geo,this,null,null,null,coords);
}
}
