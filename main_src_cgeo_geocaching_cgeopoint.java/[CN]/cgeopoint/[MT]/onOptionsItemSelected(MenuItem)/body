{
  final int menuItem=item.getItemId();
  final Geopoint coords=getDestination();
  if (coords != null) {
    addToHistory(coords);
  }
  if (menuItem == 2) {
    navigateTo();
    return true;
  }
 else   if (menuItem == 5) {
    cachesAround();
    return true;
  }
 else   if (menuItem == 6) {
    clearHistory();
    return true;
  }
  return NavigationAppFactory.onMenuItemSelected(item,geo,this,res,null,null,null,coords);
}
