{
  try {
    if (factory == null) {
      factory=new BitmapFactory();
    }
    for (int level=1; level <= 5; level++) {
      try {
        final Bitmap image=BitmapFactory.decodeFile(StaticMapsProvider.getMapFile(geocode,level).getPath());
        if (image != null) {
          maps.add(image);
        }
      }
 catch (      Exception e) {
        Log.e(Settings.tag,"cgeosmaps.loadMaps.run.1: " + e.toString());
      }
    }
    if (maps.isEmpty()) {
      for (int level=1; level <= 5; level++) {
        try {
          final Bitmap image=BitmapFactory.decodeFile(StaticMapsProvider.getMapFile(geocode,level).getPath());
          if (image != null) {
            maps.add(image);
          }
        }
 catch (        Exception e) {
          Log.e(Settings.tag,"cgeosmaps.loadMaps.run.2: " + e.toString());
        }
      }
    }
    loadMapsHandler.sendMessage(new Message());
  }
 catch (  Exception e) {
    Log.e(Settings.tag,"cgeosmaps.loadMaps.run: " + e.toString());
  }
}
