{
  inflater=((Activity)context).getLayoutInflater();
  setPersistent(false);
  if (attrs == null) {
    return;
  }
  TypedArray types=context.obtainStyledAttributes(attrs,new int[]{android.R.attr.text,R.attr.url,R.attr.urlButton},defStyle,0);
  text=types.getString(0);
  url=types.getString(1);
  urlButton=types.getString(2);
  types.recycle();
}
