{
  if (StringUtils.isBlank(prefix)) {
    setPrefix(old.prefix);
  }
  if (StringUtils.isBlank(lookup)) {
    lookup=old.lookup;
  }
  if (StringUtils.isBlank(name)) {
    setName(old.name);
  }
  if (StringUtils.isBlank(latlon) || latlon.startsWith("?")) {
    latlon=old.latlon;
  }
  if (coords == null) {
    coords=old.coords;
  }
  if (StringUtils.isBlank(note)) {
    note=old.note;
  }
  if (note != null && old.note != null) {
    if (old.note.length() > note.length()) {
      note=old.note;
    }
  }
  if (id < 0) {
    id=old.id;
  }
  visited=old.visited;
}
