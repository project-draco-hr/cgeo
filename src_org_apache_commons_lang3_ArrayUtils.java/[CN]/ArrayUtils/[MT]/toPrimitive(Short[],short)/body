{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return EMPTY_SHORT_ARRAY;
  }
  final short[] result=new short[array.length];
  for (int i=0; i < array.length; i++) {
    Short b=array[i];
    result[i]=(b == null ? valueForNull : b.shortValue());
  }
  return result;
}
