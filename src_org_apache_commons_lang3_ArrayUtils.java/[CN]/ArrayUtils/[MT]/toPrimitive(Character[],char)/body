{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return EMPTY_CHAR_ARRAY;
  }
  final char[] result=new char[array.length];
  for (int i=0; i < array.length; i++) {
    Character b=array[i];
    result[i]=(b == null ? valueForNull : b.charValue());
  }
  return result;
}
