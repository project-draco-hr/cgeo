{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return EMPTY_INT_ARRAY;
  }
  final int[] result=new int[array.length];
  for (int i=0; i < array.length; i++) {
    Integer b=array[i];
    result[i]=(b == null ? valueForNull : b.intValue());
  }
  return result;
}
