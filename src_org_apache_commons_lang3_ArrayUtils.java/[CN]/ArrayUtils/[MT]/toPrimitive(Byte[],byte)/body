{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return EMPTY_BYTE_ARRAY;
  }
  final byte[] result=new byte[array.length];
  for (int i=0; i < array.length; i++) {
    Byte b=array[i];
    result[i]=(b == null ? valueForNull : b.byteValue());
  }
  return result;
}
