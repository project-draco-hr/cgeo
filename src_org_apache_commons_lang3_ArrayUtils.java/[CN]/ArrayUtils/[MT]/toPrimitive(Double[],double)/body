{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return EMPTY_DOUBLE_ARRAY;
  }
  final double[] result=new double[array.length];
  for (int i=0; i < array.length; i++) {
    Double b=array[i];
    result[i]=(b == null ? valueForNull : b.doubleValue());
  }
  return result;
}
