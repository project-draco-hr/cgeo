{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return EMPTY_LONG_ARRAY;
  }
  final long[] result=new long[array.length];
  for (int i=0; i < array.length; i++) {
    Long b=array[i];
    result[i]=(b == null ? valueForNull : b.longValue());
  }
  return result;
}
