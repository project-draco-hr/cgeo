{
  this.bottomLeft=new Geopoint(Math.min(bottomLeft.getLatitude(),topRight.getLatitude()),Math.min(bottomLeft.getLongitude(),topRight.getLongitude()));
  this.topRight=new Geopoint(Math.max(bottomLeft.getLatitude(),topRight.getLatitude()),Math.max(bottomLeft.getLongitude(),topRight.getLongitude()));
  this.center=new Geopoint((bottomLeft.getLatitude() + topRight.getLatitude()) / 2,(bottomLeft.getLongitude() + topRight.getLongitude()) / 2);
}
