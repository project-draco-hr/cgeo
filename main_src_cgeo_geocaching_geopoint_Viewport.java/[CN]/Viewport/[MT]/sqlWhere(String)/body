{
  final String prefix=dbTable == null ? "" : (dbTable + ".");
  return new StringBuilder(prefix).append("latitude >= ").append(getLatitudeMin()).append(" and ").append(prefix).append("latitude <= ").append(getLatitudeMax()).append(" and ").append(prefix).append("longitude >= ").append(getLongitudeMin()).append(" and ").append(prefix).append("longitude <= ").append(getLongitudeMax());
}
