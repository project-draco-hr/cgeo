{
  if (itemsPre == null) {
    return;
  }
  for (  UserOverlayItemImpl item : itemsPre) {
    item.setMarker(boundCenter(item.getMarker(0)));
  }
  items.clear();
  if (itemsPre.size() > 0) {
    items=new ArrayList<UserOverlayItemImpl>(itemsPre);
  }
  setLastFocusedItemIndex(-1);
  populate();
}
