{
  Dialogs.confirm(CacheDetailActivity.this,R.string.cache_personal_note_limit,getString(R.string.cache_personal_note_truncation,GCConstants.PERSONAL_NOTE_MAX_CHARS),new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int which){
      dialog.dismiss();
      uploadPersonalNote();
    }
  }
);
}
