{
  if (progress.isShowing()) {
    showToast(res.getString(R.string.err_detail_still_working));
    return;
  }
  if (Settings.getChooseList()) {
    new StoredList.UserInterface(CacheDetailActivity.this).promptForListSelection(R.string.list_title,new Action1<Integer>(){
      @Override public void call(      final Integer selectedListId){
        storeCache(selectedListId,new StoreCacheHandler(CacheDetailActivity.this,progress));
      }
    }
,true,StoredList.TEMPORARY_LIST_ID);
  }
 else {
    storeCache(StoredList.TEMPORARY_LIST_ID,new StoreCacheHandler(CacheDetailActivity.this,progress));
  }
}
