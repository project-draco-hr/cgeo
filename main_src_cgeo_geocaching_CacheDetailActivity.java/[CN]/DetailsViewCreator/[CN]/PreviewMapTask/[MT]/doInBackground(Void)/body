{
  try {
    Bitmap image=decode(cache);
    if (image == null) {
      StaticMapsProvider.storeCachePreviewMap(cache);
      image=decode(cache);
      if (image == null) {
        return null;
      }
    }
    return ImageHelper.scaleBitmapToFitDisplay(image);
  }
 catch (  Exception e) {
    Log.w("CacheDetailActivity.PreviewMapTask",e);
    return null;
  }
}
