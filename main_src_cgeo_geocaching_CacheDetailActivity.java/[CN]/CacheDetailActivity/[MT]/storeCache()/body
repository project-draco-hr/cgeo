{
  if (progress.isShowing()) {
    showToast(res.getString(R.string.err_detail_still_working));
    return;
  }
  if (Settings.getChooseList()) {
    new StoredList.UserInterface(this).promptForListSelection(R.string.list_title,new Action1<Integer>(){
      @Override public void call(      final Integer selectedListId){
        storeCacheInList(selectedListId,new StoreCacheHandler(CacheDetailActivity.this,progress));
      }
    }
,true,cache.getLists());
  }
 else {
    storeCacheInList(StoredList.TEMPORARY_LIST.id,new StoreCacheHandler(this,progress));
  }
}
