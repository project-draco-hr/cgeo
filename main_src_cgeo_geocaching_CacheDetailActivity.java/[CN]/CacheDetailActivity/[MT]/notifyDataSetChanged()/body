{
  if (isFinishing()) {
    return;
  }
  if (search == null) {
    return;
  }
  cache=search.getFirstCacheFromResult(LoadFlags.LOAD_ALL_DB_ONLY);
  if (cache == null) {
    progress.dismiss();
    showToast(res.getString(R.string.err_detail_cache_find_some));
    finish();
    return;
  }
  cache.setChangeNotificationHandler(new ChangeNotificationHandler(this,progress));
  updateTitleBar(cache.getGeocode(),cache.getName(),cache.getType());
  initializeAndroidBeam(new ActivitySharingInterface(){
    @Override public String getUri(){
      return cache.getCgeoUrl();
    }
  }
);
  imagesList=null;
  reinitializeViewPager();
  invalidateOptionsMenuCompatible();
  progress.dismiss();
  Settings.addCacheToHistory(cache.getGeocode());
}
