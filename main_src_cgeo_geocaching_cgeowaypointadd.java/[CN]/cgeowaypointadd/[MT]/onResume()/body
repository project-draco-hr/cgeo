{
  super.onResume();
  if (geo == null) {
    geo=app.startGeo(geoUpdate);
  }
  if (id > 0) {
    if (waitDialog == null) {
      waitDialog=ProgressDialog.show(this,null,res.getString(R.string.waypoint_loading),true);
      waitDialog.setCancelable(true);
      (new loadWaypoint()).start();
    }
  }
}
